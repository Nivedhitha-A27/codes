public class sample{
    public static void main(String[] args){
       
       
        String input="nivi0987655";
        String result="";
        
        
        for(char c:input.toCharArray()){
            if((c>='0'&&c<='9')){
               result+=c;
            }
            
        }
        
        System.out.println(result);
    }
}
