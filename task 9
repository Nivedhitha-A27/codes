public class sample{
    public static void main(String[] args){
        String a="hello world";
        String b="";
        for(int i=0;i<a.length();i++){
            char d=a.charAt(i);
            if(!Character.isWhitespace(d))
            { 
                b=b+d;
              
        }
       
    }
    System.out.println(b);
    }
 }
