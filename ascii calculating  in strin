import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        System.out.println("enter your name:");
        String a=scan.next();
        
        for(int i=0;i<a.length();i++){
          char d=a.charAt(i); 
          System.out.println(d+":"+(int)d);
        }    
    
}
}
